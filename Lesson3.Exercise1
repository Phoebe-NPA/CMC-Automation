package com.cmcglobal.lesson3;

public class Exercise1 {
    public static void main(String[] args) {

        int i = 0;
        while (i <= 100) {
            if (i % 3 == 0 && i % 5 == 0) {
                System.out.println("FizzBuzz");
                i++;
            } else if (i % 5 == 0) {
                System.out.println("Buzz");
                i++;
            } else if (i % 3 == 0) {
                System.out.println("Buzz");
                i++;
            } else {
                System.out.println(i);
                i++;
            }
        }

    }
}
